<div class="container-fluid">
<div class="row">
    <label class="control-label col-sm-2">License</label>
    <div class="col-sm-10 text-left">
        <%= @vehicle.license %><br>
    </div>
</div>
<div class="row">
    <label class="control-label col-sm-2">User Email</label>
    <div class="col-sm-10 text-left"> 
    <%= @vehicle.user.email %><br>
    </div>
</div>
</div>
<%= link_to 'Edit', edit_vehicle_path(@vehicle) %>

<%= link_to 'Delete', vehicle_path(@vehicle), method: :delete, data: { confirm: 'Are you sure?'} %>